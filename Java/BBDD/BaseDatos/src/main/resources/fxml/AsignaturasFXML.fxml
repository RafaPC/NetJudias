<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="fxRoot" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AsignaturasFXMLController">
   <children>
      <Button layoutX="69.0" layoutY="333.0" mnemonicParsing="false" onAction="#handleInsert" text="Crear">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Button layoutX="198.0" layoutY="333.0" mnemonicParsing="false" onAction="#handleUpdate" text="Actualizar">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Button layoutX="388.0" layoutY="332.0" mnemonicParsing="false" onAction="#handleDelete" text="Borrar">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <ListView fx:id="fxListAsignaturas" layoutX="93.0" layoutY="24.0" onMouseClicked="#handleMouseClick" prefHeight="246.0" prefWidth="400.0" />
      <TextField fx:id="fxNombre" layoutX="93.0" layoutY="282.0" prefHeight="26.0" prefWidth="113.0" promptText="Nombre" />
      <TextField fx:id="fxCurso" layoutX="237.0" layoutY="282.0" prefHeight="26.0" prefWidth="113.0" promptText="Curso" />
      <TextField fx:id="fxCiclo" layoutX="380.0" layoutY="282.0" prefHeight="26.0" prefWidth="113.0" promptText="Ciclo" />
   </children>
</AnchorPane>
